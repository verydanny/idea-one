{
  "name": "app",
  "version": "0.0.1",
  "scripts": {
    "dev": "PORT=3002 bun --bun run vite dev",
    "build": "bun --bun run vite build",
    "preview": "PORT=3002 bun --bun run vite preview",
    "start": "PORT=3002 bun run build/index.js",
    "test": "bun --bun run test:integration && bun --bun run test:unit",
    "check": "bun --bun run svelte-kit sync && bun --bun run svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "bun --bun run svelte-kit sync && bun --bun run svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "bun --bun run prettier --check . && eslint .",
    "format": "bun --bun run prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@playwright/test": "^1.44.1",
    "@sinclair/typebox": "^0.32.31",
    "@sveltejs/enhanced-img": "^0.2.0",
    "@sveltejs/kit": "^2.5.10",
    "@sveltejs/vite-plugin-svelte": "^4.0.0-next.3",
    "@tailwindcss/forms": "^0.5.7",
    "@types/eslint": "^8.56.10",
    "appwrite": "^14.0.1",
    "autoprefixer": "^10.4.19",
    "bits-ui": "^0.21.10",
    "bun-types": "^1.1.10",
    "clsx": "^2.1.1",
    "eslint": "^9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-svelte": "^2.39.0",
    "lucide-svelte": "^0.379.0",
    "node-appwrite": "^12.0.1",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.3",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "rollup-plugin-visualizer": "^5.12.0",
    "svelte": "^5.0.0-next.143",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-check": "^3.7.1",
    "svelte-eslint-parser": "^0.36.0",
    "svelte-language-server": "^0.16.9",
    "svelte-radix": "^1.1.0",
    "sveltekit-superforms": "^2.14",
    "tailwind-merge": "file:../../tailwind-merge",
    "tailwind-variants": "^0.2.1",
    "tailwindcss": "^3.4.3",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.0.0-alpha.20",
    "vite": "^5.2.12",
    "vite-plugin-tailwind-purgecss": "^0.3.3",
    "vitest": "^1.6.0"
  },
  "overrides": {
    "@sveltejs/vite-plugin-svelte-inspector": "^3.0.0-next.2"
  },
  "type": "module",
  "engines": {
    "bun": ">=1.1.0"
  },
  "dependencies": { "vaul-svelte": "^0.3.1" }
}
